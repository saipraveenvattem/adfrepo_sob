				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Id = ID,",
				"          Upload_Date,",
				"          project_id = PROJECT_ID,",
				"          SBU_Id = SBU_ID,",
				"          Sbu_Identifier = SBU_IDENTIFIER,",
				"          Level_ID = LEVEL_ID,",
				"          Project_Identifier = PROJECT_IDENTIFIER,",
				"          tower,",
				"          Building,",
				"          Type,",
				"          Level,",
				"          Flat_No,",
				"          Door_Type,",
				"          Jamb_Width,",
				"          Orientation,",
				"          Door_Id,",
				"          Fire_Rating,",
				"          Frame,",
				"          Foam,",
				"          Architrave,",
				"          Shutter,",
				"          Ironmongery,",
				"          Installer,",
				"          level_map = LEVEL_ID,",
				"          Total,",
				"          SC_Short,",
				"          SC_NAME,",
				"          DATE_WISE_SPLIT,",
				"          LoadDateTime,",
