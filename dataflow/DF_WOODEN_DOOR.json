				"          Sbu_Identifier as string,",
				"          Level_ID as integer,",
				"          Project_Identifier as string,",
				"          tower as string,",
				"          Building as string,",
				"          Type as string,",
				"          Level as string,",
				"          Flat_No as string,",
				"          Door_Type as string,",
				"          Jamb_Width as string,",
				"          Orientation as string,",
